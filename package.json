{
  "name": "reeds-website-new",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run static:build:clean && npm run static:build:static && npm run static:build:jsxString && npm run static:build:jsxBrowser && npm run static:build:buildCompiler && npm run static:build:compile && npm run static:build:css",
    "static:build:css": "postcss css/style.css --output build/build.css --verbose",
    "static:build:jsxString": "esbuild './jsx/MyJSXStringImplementation.tsx' --outfile=tmp/jsx/MyJSXStringImplementation.js",
    "static:build:jsxBrowser": "esbuild ./jsx/MyJSXBrowserImplementation.tsx --outfile=tmp/jsx/MyJSXBrowserImplementation.js",
    "static:build:static": "mkdir -p build && cp -r static/* build/",
    "static:build:buildCompiler": "esbuild scripts/compile.tsx --outfile=tmp/scripts/compile.mjs --format=esm",
    "static:build:compile": "node tmp/scripts/compile.mjs",
    "static:build:clean": "rm -rf tmp/* && rm -rf build/*",
    "static:dev:generate-new-post": "esbuild scripts/generate-new-post.tsx --outfile=tmp/generate-new-post.mjs --format=esm && node tmp/generate-new-post.mjs",
    "static:dev:css": "npm run static:build:css -- --watch --verbose",
    "static:dev:compile": "npm run static:build:static && npm run static:build:compile -- --watch",
    "static:dev:serve": "npx http-server -c-1 build/ -o",
    "all:dev:checkTypes": "tsc --noEmit",
    "all:dev:downloadGeneratedDatabaseTypes": "npx supabase gen types typescript --linked > server/src/supabaseGeneratedTypes.ts",
    "server:dev:build": "docker build --file Dockerfile.server -t reeds-website-server .",
    "server:dev:run": "docker run -p 3001:3001 reeds-website-server",
    "server:dev:connect": "docker run -p 3001:3001 -it reeds-website-server sh",
    "server:build:deploy": "flyctl deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/chokidar": "^2.1.3",
    "@types/commonmark": "^0.27.9",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mdx": "^2.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.5.3",
    "cssnano": "^6.0.1",
    "esbuild": "0.19.9",
    "feed": "^4.2.2",
    "http-server": "^14.1.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "remark-gfm": "^4.0.0",
    "source-map": "^0.7.4",
    "supabase": "^1.127.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.3.3",
    "vfile-reporter": "^8.1.0"
  },
  "dependencies": {
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.1",
    "dayjs": "^1.11.10"
  }
}
